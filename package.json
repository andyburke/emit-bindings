{
    "name": "emit-bindings",
    "version": "2.0.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/andyburke/emit-bindings.git"
    },
    "author": {
        "name": "Andy Burke",
        "email": "aburke@bitflood.org",
        "url": "http://github.com/andyburke"
    },
    "homepage": "https://github.com/andyburke/emit-bindings",
    "description": "Emit events on DOM element interaction using data-emit attributes.",
    "keywords": [
        "event",
        "events",
        "actions",
        "dom"
    ],
    "license": "MIT",
    "main": "./index.js",
    "scripts": {
        "test": "browserify test/* | tape-run -b firefox",
        "build": "npm run bundle && npm run compress && npm run copysource",
        "bundle": "mkdir -p build/$npm_package_version/ && browserify -p bundle-collapser/plugin index.js --standalone emit --debug | derequire --to _ereq__ | exorcist build/$npm_package_version/emit-bindings.js.map > build/$npm_package_version/emit-bindings.js",
        "compress": "mkdir -p build/$npm_package_version/ && uglifyjs --screw-ie8 --stats --compress --beautify ascii_only=true,beautify=false build/$npm_package_version/emit-bindings.js --in-source-map build/$npm_package_version/emit-bindings.js.map --source-map build/$npm_package_version/emit-bindings.min.js.map --source-map-url emit-bindings.min.js.map --output build/$npm_package_version/emit-bindings.min.js",
        "copysource": "mkdir -p build/$npm_package_version/ && cp -r index.js build/$npm_package_version",
        "stats": "browserify --full-paths index.js | discify --open",
        "deps": "browserify-graph index.js"
    },
    "devDependencies": {
        "boxcutter": "^1.2.0",
        "browserify": "^13.1.0",
        "browserify-graph": "0.0.0",
        "bundle-collapser": "^1.2.1",
        "derequire": "^2.0.3",
        "disc": "^1.3.2",
        "exorcist": "^0.4.0",
        "tape": "^4.6.0",
        "tape-run": "^2.1.4",
        "uglify-js": "^2.7.0"
    },
    "dependencies": {}
}